{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && npm run wait-for-postgres && npm run migrations:up && next dev",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:dow": "docker compose -f infra/compose.yaml down",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "test": "npm run services:up && concurrently -n next,jest --hide next -k --success command-jest --kill-others \"next dev\" \"jest --runInBand --verbose\"",
    "test:watch": "jest --watchAll --runInBand",
    "migrations:create": "node-pg-migrate -m infra/migrations create",
    "migrations:up": "node-pg-migrate -m infra/migrations --envPath .env.development up",
    "wait-for-postgres": "node infra/scripts/wait-for-postgres.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "async-retry": "^1.3.3",
    "dotenv": "^16.4.4",
    "dotenv-expand": "^11.0.6",
    "next": "^13.1.6",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jest": "^29.6.2",
    "prettier": "^3.6.2"
  }
}
